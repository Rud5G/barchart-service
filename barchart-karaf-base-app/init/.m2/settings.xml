<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">

	<servers>

		<server>
			<id>karaf-barchart-server</id>
			<username>barchart-application</username>
			<password></password>
		</server>

	</servers>

	<profiles>

		<profile>

			<id>karaf-barchart-profile</id>

			<repositories>

				<repository>
					<id>karaf-barchart-server</id>
					<url>https://nexus.barchart.com/content/groups/archon</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</repository>

			</repositories>

		</profile>

	</profiles>

	<activeProfiles>

		<activeProfile>karaf-barchart-profile</activeProfile>

	</activeProfiles>

</settings>
