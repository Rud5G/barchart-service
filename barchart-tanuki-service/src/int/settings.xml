<settings>

	<profiles>

		<profile>

			<id>int-repo</id>

			<repositories>
				<repository>
					<id>local.central</id>
					<url>@localRepositoryUrl@</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</repository>
			</repositories>

			<pluginRepositories>
				<pluginRepository>
					<id>local.central</id>
					<url>@localRepositoryUrl@</url>
					<releases>
						<enabled>true</enabled>
					</releases>
					<snapshots>
						<enabled>true</enabled>
					</snapshots>
				</pluginRepository>
			</pluginRepositories>

		</profile>

	</profiles>

	<activeProfiles>

		<activeProfile>int-repo</activeProfile>

	</activeProfiles>

</settings>
